    <dimen name="button_inset_horizontal_material">0dp</dimen>
    <!-- Default inner padding within buttons -->
    <dimen name="button_padding_vertical_material">8dp</dimen>
    <dimen name="button_padding_horizontal_material">24dp</dimen>
    <!-- Default rounded corner for controls -->
    <dimen name="control_corner_material">2dp</dimen>
